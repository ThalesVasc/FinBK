generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int    @id @default(autoincrement())
  nome     String
  email    String @unique
  senha    String
}

model Produto {
  id           Int     @id @default(autoincrement())
  nome         String
  preco        Float
  codigo_barras String
}
